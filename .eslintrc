{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "rules": {
    "max-len": [
      "error",
      150
    ],
    "linebreak-style": 0,
    "no-console": 0,
    "indent": [
      "error",
      2
    ],
    "no-unused-vars": "warn"
  }
}